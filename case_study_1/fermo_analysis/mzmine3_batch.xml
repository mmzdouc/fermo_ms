        <parameter name="Retention time" selected="false"/>
        <parameter name="features duration range" selected="true">
            <min>0.3</min>
            <max>3.0</max>
        </parameter>
        <parameter name="Chromatographic FWHM" selected="true">
            <min>0.15</min>
            <max>1.0</max>
